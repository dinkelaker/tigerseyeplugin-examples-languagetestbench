package de.tud.stg.tigerseye.examples.logo


/**
 * Tigerseye language: de.tud.stg.tigerseye.lang.logo.SimpleLogo
 *
 * Declared keywords:
 *  void backward(int)
 *  Object eval(HashMap, Closure)
 *  void forward(int)
 *  int getBlack()
 *  int getBlue()
 *  TurtleGraphicsWindow getCanvas()
 *  Object getDEBUG()
 *  int getGreen()
 *  int getRed()
 *  Turtle getTurtle()
 *  int getWhite()
 *  int getYellow()
 *  void left(int)
 *  void right(int)
